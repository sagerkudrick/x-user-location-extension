{
  "manifest_version": 3,
  "name": "User Location Fetcher",
  "version": "1.0",
  "permissions": ["cookies", "storage", "scripting", "activeTab"],
  "host_permissions": ["https://x.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://x.com/*"],
      "js": ["content.js"]
    }
  ]
}
